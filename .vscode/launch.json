    "version": "0.2.0",
    "configurations": [
    {
        "type": "cortex-debug",
        "request": "launch",
        "name": "Debug stm32",
        "cwd": "${workspaceRoot}",
        "executable": "${workspaceRoot}/build/stm32l4-app.elf",
        "servertype": "openocd",
        "configFiles": [
            "interface/stlink-v2-1.cfg",
            "target/stm32l4x.cfg",
        ],
        "gdbPath": "/usr/bin/gdb-multiarch",
        // "armToolchainPath": "/usr/bin/",
        // "device": "...",
        "svdFile": "STM32L4x6.svd",
        "showDevDebugOutput": "raw", 
    },
    ]
}